#include <stdio.h>

int main(void) {

 
//NOMES PARA O CALCULO


float R1_2;// REQ  R1 COM R2
float R1_2_3;// REQ  R1_2 COM R3

  //Declarações de variaveis
float e_R1, e_R2, e_R3, e_R4;
float s_Req;

//entrada do valor
printf("Valor de R1:");
scanf ("%f",&e_R1);

//entrada do valor 
printf("Valor de R2:");
scanf ("%f",& e_R2);

//entrada do valor 
printf("Valor de R3:");
scanf ("%f",& e_R3);

//entrada do valor 
printf("Valor de R4:");
scanf ("%f",& e_R4);



//atribuição: 

R1_2 = ((e_R1*e_R2)/(e_R1+ e_R2));

R1_2_3 = ((R1_2*e_R3)/(R1_2+ e_R3));

s_Req = ((R1_2_3*e_R4)/(R1_2_3+ e_R4));

 
 if (s_Req == 0)
 
 {
   printf("Erro de Calculo:");
 }

else {


printf("Req: %f [Ohms]\n",
s_Req);

}
  return 0;
}
