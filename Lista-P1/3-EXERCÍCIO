#include <stdio.h>

int main(void) {

//NOMES PARA O CALCULO

float e_A, e_B; 

float s_A, s_B; 

//Declarações de variaveis
//float  A , B;

//entrada do valor
printf("Valor do A:");
scanf ("%f",&e_A);

//entrada do valor
printf("Valor do B:");
scanf ("%f",&e_B);


//atribuição:

//Vice-Versa
 
 s_A= e_B;

 s_B= e_A;

{


 
printf("s_A: %f [A]\n",
s_A);

 
printf("s_B: %f [A]\n",
s_B);

}

  return 0;
}
